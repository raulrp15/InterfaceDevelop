<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CRUD.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:CRUD"
    xmlns:local2="clr-namespace:CRUD.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="CRUD">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local2:ListadoPersonas}"
        Route="Listado" />

    <ShellContent
    Title="Home"
    ContentTemplate="{DataTemplate local2:InsertarPage}"
    Route="Insertar" />

</Shell>
